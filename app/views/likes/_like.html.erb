<% if user_signed_in? && tweet.liked_by?(current_user)%>
  <%=link_to "いいね済", tweet_likes_path(tweet.id),  method: :delete, class: "tweet_likes" ,value: tweet.id, data:{atr:"true"} , remote: true %>
<% else %>
  <%=link_to "いいねする", tweet_likes_path(tweet.id), method: :post, class: "tweet_likes" ,value: tweet.id,data:{atr:"false"} , remote: true %>
<% end %>